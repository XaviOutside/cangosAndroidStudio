<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cangos"
    android:versionCode="8"
    android:versionName="1.0" >

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Main.MainCangos"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity
            android:name=".Users.MainUsers"
            android:label="@string/title_activity_main_users" >
        </activity>

        <activity
            android:name=".Users.ListUsers"
            android:label="@string/title_activity_main_users" >
        </activity>

        <activity 
            android:name=".Pets.MainPets"
            android:label="@string/title_activity_main_pets">
        </activity>

        <activity
            android:name=".Pets.ListPets"
            android:label="@string/title_activity_list_pets">
        </activity>

        <activity
            android:name=".Services.MainServices"
            android:label="@string/title_activity_main_services">
        </activity>

        <activity
            android:name=".Services.ListServices"
            android:label="@string/title_activity_list_services">
        </activity>
    </application>

</manifest>